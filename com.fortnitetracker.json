{
    "module_type": "org.thingpedia.v2",
    "params": {},
    "category": "data",
    "subcategory": "service",
    "types": [],
    "child_types": [],
    "auth": {
        "type": "none"
    },
    "queries": {
        "get": {
            "args": [
                {
                    "name": "user_id",
                    "type": "String",
                    "question": "what's the user id you want to track?",
                    "is_input": true,
                    "required": true
                },
                {
                    "name": "rating",
                    "type": "Number",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "rank",
                    "type": "Number",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "kd",
                    "type": "Number",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "winRatio",
                    "type": "Number",
                    "question": "",
                    "is_input": false,
                    "required": false
                }
            ],
            "doc": "Get the stats of a user",
            "confirmation": "fortnite stats of $user_id",
            "confirmation_remote": "",
            "canonical": "get fortnite stats",
            "formatted": [
                {
                    "type": "text",
                    "text": "Stats for ${user_id}: "
                },
                {
                    "type": "text",
                    "text": "rating: ${rating}; ranking: ${rank}; k/d ratio: ${kd}; win ratio: ${winRatio}"
                }
            ],
            "poll_interval": 3600000,
            "is_list": false
        }
    },
    "actions": {},
    "examples": [
        {
            "utterance": "get fortnite stats of $p_user_id",
            "program": "let table x := \\(p_user_id :String) -> @com.fortnitetracker.get(user_id=p_user_id);"
        },
        {
            "utterance": "show me the fortnite stats ",
            "program": "let table x := @com.fortnitetracker.get();"
        },
        {
            "utterance": "get me $p_user_id's stats on game fortnite",
            "program": "let table x := \\(p_user_id :String) -> @com.fortnitetracker.get(user_id=p_user_id);"
        }
    ]
}
