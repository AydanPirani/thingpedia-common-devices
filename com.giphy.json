{
  "module_type": "org.thingpedia.v2",
  "params": {},
  "category": "data",
  "subcategory": "media",
  "types": [],
  "child_types": [],
  "auth": {
    "type": "none"
  },
  "triggers": {},
  "actions": {},
  "queries": {
    "get": {
      "args": [
        {
          "name": "tag",
          "type": "Hashtag",
          "question": "With what tag?",
          "is_input": true,
          "required": false
        },
        {
          "name": "picture_url",
          "type": "Picture",
          "question": "",
          "is_input": false,
          "required": false
        }
      ],
      "doc": "get a gif from Giphy, optionally with the given tag",
      "confirmation": "get a gif from giphy",
      "confirmation_remote": "",
      "canonical": "random giphy gif",
      "formatted": [
        {
          "type": "picture",
          "url": "${picture_url}"
        }
      ],
      "poll_interval": -1
    }
  },
  "examples": [
    {
      "utterance": "a random gif from giphy",
      "program": "let table x := \\() -> @com.giphy.get();"
    },
    {
      "utterance": "a gif with tag ${p_tag} from giphy",
      "program": "let table x := \\(p_tag :Hashtag) -> @com.giphy.get(tag=p_tag);"
    },
    {
      "utterance": "a ${p_tag:const} gif",
      "program": "let table x := \\(p_tag :Hashtag) -> @com.giphy.get(tag=p_tag);"
    }
  ]
}
