{
    "module_type": "org.thingpedia.v2",
    "params": {},
    "category": "data",
    "subcategory": "media",
    "types": [],
    "child_types": [],
    "auth": {
        "type": "none"
    },
    "queries": {
        "get": {
            "args": [
                {
                    "name": "tag",
                    "type": "Entity(tt:hashtag)",
                    "question": "With what tag?",
                    "is_input": true,
                    "required": false
                },
                {
                    "name": "picture_url",
                    "type": "Entity(tt:picture)",
                    "question": "",
                    "is_input": false,
                    "required": false
                }
            ],
            "doc": "get a gif from Giphy, optionally with the given tag",
            "confirmation": "a gif from giphy",
            "confirmation_remote": "",
            "canonical": "random giphy gif",
            "formatted": [
                {
                    "type": "picture",
                    "url": "${picture_url}"
                }
            ],
            "poll_interval": -1,
            "is_list": false
        }
    },
    "actions": {},
    "examples": [
        {
            "utterance": "a random gif from giphy",
            "program": "let table x := @com.giphy.get();"
        },
        {
            "utterance": "a gif with tag ${p_tag} from giphy",
            "program": "let table x := \\(p_tag :Entity(tt:hashtag)) -> @com.giphy.get(tag=p_tag);"
        },
        {
            "utterance": "a ${p_tag:const} gif",
            "program": "let table x := \\(p_tag :Entity(tt:hashtag)) -> @com.giphy.get(tag=p_tag);"
        },
        {
            "utterance": "a random gif",
            "program": "let table x := @com.giphy.get();"
        },
        {
            "utterance": "a ${p_tag:const} picture",
            "program": "let table x := \\(p_tag :Entity(tt:hashtag)) -> @com.giphy.get(tag=p_tag);"
        },
        {
            "utterance": "a gif tagged ${p_tag}",
            "program": "let table x := \\(p_tag :Entity(tt:hashtag)) -> @com.giphy.get(tag=p_tag);"
        },
        {
            "utterance": "a ${p_tag:const} animated picture",
            "program": "let table x := \\(p_tag :Entity(tt:hashtag)) -> @com.giphy.get(tag=p_tag);"
        },
        {
            "utterance": "a gif about ${p_tag:const}",
            "program": "let table x := \\(p_tag :Entity(tt:hashtag)) -> @com.giphy.get(tag=p_tag);"
        },
        {
            "utterance": "a picture about ${p_tag:const}",
            "program": "let table x := \\(p_tag :Entity(tt:hashtag)) -> @com.giphy.get(tag=p_tag);"
        }
    ]
}
