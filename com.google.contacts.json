{
    "module_type": "org.thingpedia.v2",
    "params": {},
    "category": "online",
    "subcategory": "social-network",
    "types": [],
    "child_types": [],
    "auth": {
        "type": "oauth2"
    },
    "queries": {
        "get_contacts": {
            "args": [
                {
                    "name": "name",
                    "type": "String",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "email",
                    "type": "Entity(tt:email_address)",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "phone_number",
                    "type": "Entity(tt:phone_number)",
                    "question": "",
                    "is_input": false,
                    "required": false
                }
            ],
            "doc": "Gets a user's Google contacts",
            "confirmation": "retrieve your Google contacts",
            "confirmation_remote": "retrieve $_person's Google contacts",
            "canonical": "get my Google contacts",
            "formatted": [
                {
                    "type": "text",
                    "text": "Name: ${name}"
                },
                {
                    "type": "text",
                    "text": "Email: ${email}"
                },
                {
                    "type": "text",
                    "text": "Phone Number: $phone_number"
                }
            ],
            "poll_interval": 86400000,
            "is_list": true
        }
    },
    "actions": {},
    "examples": [
        {
            "utterance": "my google contacts",
            "program": "let table x := @com.google.contacts.get_contacts();"
        },
        {
            "utterance": "my contacts",
            "program": "let table x := @com.google.contacts.get_contacts();"
        },
        {
            "utterance": "my contacts on google",
            "program": "let table x := @com.google.contacts.get_contacts();"
        },
        {
            "utterance": "my contact's info",
            "program": "let table x := @com.google.contacts.get_contacts();"
        },
        {
            "utterance": "get info on people I know",
            "program": "let table x := @com.google.contacts.get_contacts();"
        },
        {
            "utterance": "my contacts with email ${p_email}",
            "program": "let table my_contacts_with_email := \\(p_email :Entity(tt:email_address)) -> (@com.google.contacts.get_contacts()), email == p_email;"
        },
        {
            "utterance": "my contacts with phone number ${p_phone_number}",
            "program": "let table my_contacts_with_phone_number := \\(p_phone_number :Entity(tt:phone_number)) -> (@com.google.contacts.get_contacts()), phone_number == p_phone_number;"
        },
        {
            "utterance": "my contacts with name ${p_name}",
            "program": "let table my_contacts_with_name := \\(p_name :String) -> (@com.google.contacts.get_contacts()), name == p_name;"
        },
        {
            "utterance": "my google contacts with email ${p_email}",
            "program": "let table my_contacts_with_email := \\(p_email :Entity(tt:email_address)) -> (@com.google.contacts.get_contacts()), email == p_email;"
        },
        {
            "utterance": "my contacts with phone number ${p_phone_number} and email ${p_email}",
            "program": "let table my_contacts_with_phone_number := \\(p_phone_number :Entity(tt:phone_number), p_email :Entity(tt:email_address)) -> (@com.google.contacts.get_contacts()), (phone_number == p_phone_number && email == p_email);"
        }
    ]
}
