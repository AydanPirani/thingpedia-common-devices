{
  "auth": {
    "type": "oauth2"
  },
  "types": [
    "online-account"
  ],
  "global-name": "google",
  "params": {},
  "triggers": {
    "receive_email": {
      "args": [
        "from",
        "subject",
        "messageid"
      ],
      "schema": [
        "String",
        "String",
        "String"
      ],
      "doc": "receive email from others"
    },
    "new_drive_file": {
      "args": [
        "filename"
      ],
      "schema": [
        "String"
      ],
      "doc": "check if there is a new file/folder created"
    }
  },
  "actions": {
    "trash_email": {
      "args": [
        "messageid"
      ],
      "schema": [
        "String"
      ],
      "doc": "trash the specified email"
    },
    "create_new_drive_file": {
      "args": [
        "name"
      ],
      "schema": [
        "String"
      ],
      "doc": "create a new file with the specified name"
    }
  }
}
