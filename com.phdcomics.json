{
  "module_type": "org.thingpedia.v2",
  "params": {},
  "category": "data",
  "subcategory": "media",
  "types": [],
  "child_types": [],
  "auth": {
    "type": "none"
  },
  "triggers": {},
  "actions": {},
  "queries": {
    "get_post": {
      "args": [
        {
          "name": "title",
          "type": "String",
          "question": "",
          "is_input": false,
          "required": false
        },
        {
          "name": "link",
          "type": "URL",
          "question": "",
          "is_input": false,
          "required": false
        },
        {
          "name": "picture_url",
          "type": "Picture",
          "question": "",
          "is_input": false,
          "required": false
        },
        {
          "name": "updated_time",
          "type": "Date",
          "question": "",
          "is_input": false,
          "required": false
        }
      ],
      "doc": "get the latest phd comics posts",
      "confirmation": "get PhD comics",
      "confirmation_remote": "",
      "canonical": "phdcomics posts",
      "formatted": [
        {
          "type": "rdl",
          "displayTitle": "${title}",
          "webCallback": "${link}"
        },
        {
          "type": "picture",
          "text": "${picture_url}"
        }
      ],
      "poll_interval": 10800000
    }
  },
  "examples": [
    {
      "utterance": "when there is a new post on phd comics",
      "program": "let stream x := \\() -> monitor @com.phdcomics.get_post();"
    },
    {
      "utterance": "posts on phd comics",
      "program": "let table x := \\() -> @com.phdcomics.get_post();"
    },
    {
      "utterance": "piled higher and deeper comics",
      "program": "let table x := \\() -> @com.phdcomics.get_post();"
    }
  ]
}
