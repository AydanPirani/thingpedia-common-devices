{
  "module_type": "org.thingpedia.v2",
  "kind": "com.wikicfp",
  "params": {},
  "auth": {
    "type": "none"
  },
  "queries": {
    "search": {
      "args": [
        {
          "name": "query",
          "type": "String",
          "question": "What do you want to search?",
          "is_input": true,
          "required": true,
          "string_values": "tt:search_query"
        },
        {
          "name": "abbr",
          "type": "String",
          "question": "",
          "is_input": false,
          "required": false
        },
        {
          "name": "name",
          "type": "String",
          "question": "",
          "is_input": false,
          "required": false
        },
        {
          "name": "start",
          "type": "Date",
          "question": "",
          "is_input": false,
          "required": false
        },
        {
          "name": "end",
          "type": "Date",
          "question": "",
          "is_input": false,
          "required": false
        },
        {
          "name": "city",
          "type": "String",
          "question": "",
          "is_input": false,
          "required": false
        },
        {
          "name": "deadline",
          "type": "Date",
          "question": "",
          "is_input": false,
          "required": false
        }
      ],
      "canonical": "get events from wikicfp",
      "is_list": true,
      "poll_interval": 3600000,
      "confirmation": "information of $query on wikicfp",
      "formatted": [
        {
          "type": "text",
          "text": "${name} (${abbr})"
        },
        {
          "type": "text",
          "text": "When: ${start:date} - ${end:date}"
        },
        {
          "type": "text",
          "text": "Where: ${city}"
        },
        {
          "type": "text",
          "text": "Deadline: ${deadline:date}"
        }
      ],
      "doc": "get conference details"
    }
  },
  "actions": {},
  "version": 5,
  "types": [],
  "child_types": [],
  "category": "data",
  "name": "WikiCFP",
  "description": "Keep updated with paper submission deadlines"
}
