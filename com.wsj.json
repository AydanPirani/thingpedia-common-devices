{
    "module_type": "org.thingpedia.v2",
    "params": {},
    "category": "data",
    "subcategory": "media",
    "types": [],
    "child_types": [],
    "auth": {
        "type": "none"
    },
    "queries": {
        "get": {
            "args": [
                {
                    "name": "section",
                    "type": "Enum(opinions,world_news,us_business,markets,technology,lifestyle)",
                    "question": "What section of the Wall Street Journal do you want to read?",
                    "is_input": true,
                    "required": true
                },
                {
                    "name": "title",
                    "type": "String",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "link",
                    "type": "Entity(tt:url)",
                    "question": "",
                    "is_input": false,
                    "required": false
                },
                {
                    "name": "updated",
                    "type": "Date",
                    "question": "",
                    "is_input": false,
                    "required": false
                }
            ],
            "doc": "trigger on new articles",
            "confirmation": "articles published in the $section section of the Wall Street Journal",
            "confirmation_remote": "",
            "canonical": "get post on wall street journal",
            "formatted": [
                {
                    "type": "rdl",
                    "webCallback": "${link}",
                    "displayTitle": "${title}"
                }
            ],
            "poll_interval": 1080000,
            "is_list": true
        }
    },
    "actions": {},
    "examples": [
        {
            "utterance": "when there is a new article in wsj $p_section section",
            "program": "let stream x := \\(p_section :Enum(opinions,world_news,us_business,markets,technology,lifestyle)) -> monitor (@com.wsj.get(section=p_section));"
        },
        {
            "utterance": "wsj $p_section articles",
            "program": "let table x := \\(p_section :Enum(opinions,world_news,us_business,markets,technology,lifestyle)) -> @com.wsj.get(section=p_section);"
        },
        {
            "utterance": "wall street journal articles",
            "program": "let table x := @com.wsj.get();"
        },
        {
            "utterance": "wall street journal articles in the $p_section section",
            "program": "let table x := \\(p_section :Enum(opinions,world_news,us_business,markets,technology,lifestyle)) -> @com.wsj.get(section=p_section);"
        },
        {
            "utterance": "wall street journal articles in the $p_section section published in the last hour",
            "program": "let table x := \\(p_section :Enum(opinions,world_news,us_business,markets,technology,lifestyle)) -> (@com.wsj.get(section=p_section)), updated >= start_of(h);"
        },
        {
            "utterance": "when there is a new article in the wall street journal $p_section section",
            "program": "let stream x := \\(p_section :Enum(opinions,world_news,us_business,markets,technology,lifestyle)) -> monitor (@com.wsj.get(section=p_section));"
        },
        {
            "utterance": "when the wall street journal publishes a new article in the $p_section section",
            "program": "let stream x := \\(p_section :Enum(opinions,world_news,us_business,markets,technology,lifestyle)) -> monitor (@com.wsj.get(section=p_section));"
        },
        {
            "utterance": "wall street journal $p_section",
            "program": "let table x := \\(p_section :Enum(opinions,world_news,us_business,markets,technology,lifestyle)) -> @com.wsj.get(section=p_section);"
        }
    ],
    "name": "The Wall Street Journal",
    "description": "Breaking News, Business, Financial and Economic News, World News and Video"
}
