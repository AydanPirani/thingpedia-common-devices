{
    "module_type": "org.thingpedia.generic_rest.v1",
    "params": {},
    "category": "data",
    "subcategory": "media",
    "types": [],
    "child_types": [],
    "auth": {
        "type": "none"
    },
    "queries": {
        "get": {
            "args": [
                {
                    "name": "count",
                    "type": "Number",
                    "question": "How many dogs do you want?",
                    "is_input": true,
                    "required": false
                },
                {
                    "name": "image_id",
                    "type": "Entity(dogapi:image_id)",
                    "question": "",
                    "is_input": false,
                    "required": false,
                    "json_key": "id"
                },
                {
                    "name": "picture_url",
                    "type": "Entity(tt:picture)",
                    "question": "",
                    "is_input": false,
                    "required": false,
                    "json_key": "url"
                }
            ],
            "doc": "get `count` many dog pictures",
            "confirmation": "get dog pictures",
            "confirmation_remote": "",
            "canonical": "get dogs",
            "formatted": [
                {
                    "type": "picture",
                    "url": "${picture_url}"
                }
            ],
            "poll_interval": -1,
            "is_list": false,
            "url": "https://api.thedogapi.co.uk/v2/dog.php?limit=${count}",
            "json_key": "data"
        }
    },
    "actions": {},
    "examples": [
        {
            "utterance": "dog pictures",
            "program": "let table x := @uk.co.thedogapi.get();"
        },
        {
            "utterance": "puppy pictures",
            "program": "let table x := @uk.co.thedogapi.get();"
        },
        {
            "utterance": "a random picture of a dog",
            "program": "let table x := @uk.co.thedogapi.get();"
        },
        {
            "utterance": "a picture of a random dog",
            "program": "let table x := @uk.co.thedogapi.get();"
        },
        {
            "utterance": "${p_count:const} dog pictures",
            "program": "let table x := \\(p_count :Number) -> @uk.co.thedogapi.get(count=p_count);"
        }
    ],
    "name": "The Dog API",
    "description": "Random Doggo Pictures"
}
